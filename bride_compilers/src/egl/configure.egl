#!/usr/bin/env python
import roslib;roslib.load_manifest('[%=package.name%]')

from dynamic_reconfigure.parameter_generator import *

gen = ParameterGenerator()

[%for (param in node.parameter) {%]
gen.add("[%=param.name%]", [%=param.type%], 0, "Autogenerated parameter based on model.", [%=param.value%], 0, 100)
[%}%]

exit(gen.generate("[%=package.name%]", "[%=package.name%]", "[%=node.name%]"))